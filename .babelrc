{
    "presets": [
        ["@babel/preset-env",
            {
                "corejs": { "version":3 },
                "useBuiltIns": "usage"
            }
        ],
        ["@babel/preset-react"]
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators",{"decoratorsBeforeExport":true}],
        ["@babel/plugin-proposal-class-properties"],
        ["@babel/transform-runtime"],
        ["transform-remove-console"]
    ]
}